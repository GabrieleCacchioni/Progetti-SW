<parameters>
	<frame>
		<width> 500 </width>
		<height> 800 </height>
	</frame>
	
	<frame_default>
		<width> 300 </width>
		<height> 300 </height>
	</frame_default>
	
	<buttons>	
		<button id="start">
			<tooltip expr="lancia evento START" />
			<color expr="RED" />
		</button>
		
		<button id="meno">
			<tooltip expr="lancia evento MENO" />
		</button>
				
		<button id="piu">
			<tooltip expr="" />
		</button>
		
		<button id="watch_reset">
			<tooltip expr="reset" />
			<color expr="" />
		</button>
		

	</buttons>
	
	<button_default>
		<dtooltip expr="This is an event" />
		<dcolor expr="LIGHT_GRAY" />
	</button_default>
	
	
	<panels>
		<panel id="totale">
			<color expr="RED" />
			<text_field_color expr="GREEN" />
			<text_field_tooltip expr="running" />
		</panel>
		
		<panel id="paused$value">
			<color expr="" />
			<tooltip expr="paused value" />
			<text_field_color expr="GRAY" />
			<text_field_tooltip expr="paused" />
		</panel>
		
		<panel id="stopped$value">
			<color expr="YELLOW" />
			<tooltip expr="stopped value" />
			<text_field_tooltip expr="stopped" />
		</panel>
		
	</panels>
	
	<panel_default>
		<dcolor expr="LIGHT_GRAY"/>
		<dtooltip expr="This is a variable" />
		<dtext_field_color expr="WHITE"/>
		<dtext_field_tooltip expr="variable" />
	</panel_default>
	
	<fonts>
		<font id = "buttons">
			<type expr="Helvetica"/>
			<mode expr="BOLD" />
			<dimension expr="18" />
		</font>
		
		<font id="fields">
			<type expr="" />
			<dimension expr="16" />
		</font>
	</fonts>
	
	
	<font_default>
		<dtype expr="TimesRoman" />
		<dmode expr="PLAIN" />
		<ddimension expr="14" />
	</font_default>

	
	

</parameters>