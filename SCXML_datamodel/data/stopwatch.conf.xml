<parameters>
	<frame>
		<width> 500 </width>
		<height> 800 </height>
	</frame>
	
	<defaultf>
		<width> 300 </width>
		<height> 300 </height>
	</defaultf>
	
	<buttons>	
		<button id="watch_start">
			<btooltip expr="start" />
			<bcolor expr="RED" />
		</button>
		
		<button id="watch_split">
			<btooltip expr="split" />
		</button>
				
		<button id="watch_unsplit">
			<btooltip expr="" />
			<bcolor expr="WHITE" />
		</button>
		
		<button id="watch_reset">
			<btooltip expr="reset" />
			<bcolor expr="" />
		</button>
		

	</buttons>
	
	<defaultb>
		<dtooltip expr="This is an event" />
		<dcolor expr="LIGHT_GRAY" />
	</defaultb>
	
	
	<panels>
		<panel id="running$value">
			<pcolor expr="RED" />
			<tfcolor expr="GREEN" />
			<tftooltip expr="running" />
		</panel>
		
		<panel id="paused$value">
			<pcolor expr="" />
			<ptooltip expr="paused value" />
			<tfcolor expr="GRAY" />
			<tftooltip expr="paused" />
		</panel>
		
		<panel id="stopped$value">
			<pcolor expr="YELLOW" />
			<ptooltip expr="stopped value" />
			<tftooltip expr="stopped" />
		</panel>
		
	</panels>
	
	<defaultp>
		<dcolor expr="LIGHT_GRAY"/>
		<dtooltip expr="This is a variable" />
		<dtfcolor expr="WHITE"/>
		<dtftooltip expr="variable" />
	</defaultp>
	
	<fonts>
		<font id = "buttons">
			<type expr="Helevetica"/>
			<mode expr="BOLD" />
			<dimension expr="18" />
		</font>
		
		<font id="fields">
			<type expr="" />
			<dimension expr="16" />
		</font>
	</fonts>
	
	
	<defaultcar>
		<dtype expr="TimesRoman" />
		<dmode expr="PLAIN" />
		<ddimension expr="14" />
	</defaultcar>

	
	

</parameters>