<parameters>
	<frame>
		<width> 500 </width>
		<height> 800 </height>
	</frame>
	
	<buttons>	
		<button id="watch_start">
			<tooltip expr="start" />
			<color expr="RED" />
		</button>
		
		<button id="split">
			<tooltip expr="split" />
			<color expr="BLUE" />
		</button>
		
		<button id="stop">
			<tooltip expr="Stop" />
			<color expr="YELLOW" />
		</button>
		
		<button id="unsplit">
			<tooltip expr="" />
			<color expr="WHITE" />
		</button>
		
		<button id="reset">
			<tooltip expr="reset" />
			<color expr="" />
		</button>
	
	</buttons>
	
	<defaultb>
		<dtooltip expr="This is an event" />
		<dcolor expr="WHITE" />
	</defaultb>
	

</parameters>